<!DOCTYPE html>
<html>
  <head>
    <title>Vertical</title>
  </head>
  <body>
    <h3>Enter search:</h3>
    <form action="/vertical" method="GET">
        <input type="text" name="search">
        <input type="submit">
    </form>
    <h3>Enter browse:</h3>
    <form action="/vertical" method="GET">
        <select name="browse">
            <% data.forEach(function(category) { %>
                <option value="<%= category.category_id %>"><%= category.category_name %></option>
            <% }); %>
        </select>
        <input type="submit">
    </form>
  </body>
</html>
